{
  "expo": {
    "name": "QR Guru",
    "slug": "qrguru",
    "version": "1.4.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.qrguru.qrguru",
      "buildNumber": "6"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#000"
      },
      "package": "com.qrguru.qrguru",
      "versionCode": 6
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c68f5747-460f-400c-b39c-664b2ed20208"
      }
    },
    "owner": "qr-guru",
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "The app needs to access your camera to scan QR codes"
        }
      ]
    ]
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-5499479031752321~6963395025",
    "ios_app_id": "ca-app-pub-5499479031752321~3152298051"
  }
}
